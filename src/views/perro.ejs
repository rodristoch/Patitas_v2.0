            <!---- Contenedor de productos ---->
            <div class="lista_desktop">
                <div class="div-lista-desktop">

                    <!-- <h2 id="h2tituloGato" class="h2-titulo">Productos Gato</h2> -->

                    <div class="lista_flexbox_desktop"> <!-- div visible -->

                        <h2 id="h2tituloPerro" class="h2-titulo" style="width: 100%;">Productos Perro</h2>  <!-- el titulo q se ve en MOBILE -->

                        <% for(let producto of perro) { %>
                            <% if(producto.descuentos && producto.descuentos[0] && producto.descuentos[0].id == 2) { %> <!-- si tienen descuentos los productos -->
                                <article class="articulo-productos-descuentos"> <!-- las cards con borde azul -->
                            <% } else { %> <!-- si no tienen descuentos los productos -->
                                <article class="articulo-productos"> <!-- las cards con borde normal -->
                            <% } %>
                                <!-- para saber el contenido de la etiqueta -->
                                <div class="div-etiqueta">
                                    <% if(producto.categorias[0].id == 1) { %>
                                        <p class="etiqueta-comida-bicho-card"><%= producto.categorias[0].categoria %></p>
                                    <% } else if(producto.categorias[0].id == 2) { %>
                                        <p class="etiqueta-accesorio-bicho-card"><%= producto.categorias[0].categoria %></p>
                                    <% } else if(producto.categorias[0].id == 3) { %>
                                        <p class="etiqueta-higiene-bicho-card"><%= producto.categorias[0].categoria %></p>
                                    <% } else if(producto.categorias[0].id == 4) { %>
                                        <p class="etiqueta-juguete-bicho-card"><%= producto.categorias[0].categoria %></p>
                                    <% } %>
                                </div>
                                <!-- hasta acá etiqueta -->
                                <div class="contenedor_imagen">
                                    <!-- dependiendo de la categoria q sea el producto, para q te lleve a la ruta de categoria correspondiente -->
                                    <% if(producto.categorias && producto.categorias[0]) { %> 
                                        <% if(producto.categorias[0].categoria == "comida") { %> 
                                            <a href="/productos/perro/comida/<%= producto.id %>">
                                        <% } else if (producto.categorias[0].categoria == "accesorio") { %>  
                                            <a href="/productos/perro/accesorios/<%= producto.id %>">
                                        <% } else if (producto.categorias[0].categoria == "higiene") { %>
                                            <a href="/productos/perro/higiene/<%= producto.id %>">
                                        <% } else if (producto.categorias[0].categoria == "juguete") { %>
                                            <a href="/productos/perro/juguetes/<%= producto.id %>">
                                        <% } %>
                                                <img class="img-productos-descuentos" src="/img/productos/<%= producto.imagen %>" alt="imagen_producto">
                                            </a>
                                    <% } else { %>
                                            <a href="/productos/perro/comida/<%= producto.id %>">
                                                <img class="img-productos-descuentos" src="/img/productos/<%= producto.imagen %>" alt="imagen_producto">
                                            </a>
                                    <% } %>
                                </div>
                                <div class="div-card">
                                    <div class="texto_nombre_arriba">
                                        <h4><%= producto.nombre %></h4>
                                    </div>
                                    <div class="texto_descripcion_arriba">
                                        <P class="descripcion"><%= producto.descripcion %></P>
                                    </div>
                                    <% if(producto.descuentos && producto.descuentos[0] && producto.descuentos[0].id == 2) { %> <!-- si tienen descuentos los productos -->
                                        <div class="div-tres">
                                            <div class="div-tres-int">
                                                <p class="precio-tachado">$ <%= producto.precio %></p>
                                                <p class="precio">$ <%= Number((producto.precio*0.8).toFixed(2)) %></p>
                                                <p class="descuento">20% OFF</p>
                                            </div>
                                            <div class="div-dos">
                                                <p class="descuento-descrip"><%= producto.descuentos[0].descripcion %></p>
                                            </div>
                                            <div class="div-boton-comprar">
                                                <% if(userALoguearse){ %>
                                                    <button class="boton-comprar" id="comprar" type="submit" data-id="<%= producto.id %>">Agregar</button>
                                                <% } else { %>
                                                    <a href="/users/login"><button class="boton-comprarSinAuth" type="submit">Agregar</button></a>
                                                <% } %>
                                            </div>
                                        </div>
                                    <% } else { %> <!-- si no tienen descuentos los productos -->
                                        <div class="div-tres">
                                            <div class="div-tres-int">
                                                <p class="precio-tachado" style="color: white;">$ <%= producto.precio %></p>
                                                <p class="precio">$ <%= producto.precio %></p>
                                                <p class="descuento" style="color: white;">20% OFF</p>
                                            </div>
                                            <div class="div-dos">
                                                <p class="descuento-descrip" style="color: white;"><%= producto.descuentos[0].descripcion %></p>
                                            </div>
                                            <div class="div-boton-comprar">
                                                <% if(userALoguearse){ %>
                                                    <button class="boton-comprar" id="comprar" type="submit" data-id="<%= producto.id %>">Agregar</button>
                                                <% } else { %>
                                                    <a href="/users/login"><button class="boton-comprarSinAuth" type="submit">Agregar</button></a>
                                                <% } %>
                                            </div>
                                        </div>
                                    <% } %>
                                </div>
                            </article>     
                        <% } %>

                        <!-- boton de mostrar mas dentro del div visible --> 
                        <!-- <div class="div-btnver" style="width: 100%; margin: 0; text-align: center;">
                            <button class="btnver" id="btnver3"><i class="fa-solid fa-grip-vertical"></i> Mostrar mas</button>
                        </div> -->
                    </div>


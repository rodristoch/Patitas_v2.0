                            </div>
                        </section>

                        <section class="lista_filtros_desktop">
                            <h4>Precio</h4>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p">0 a $10</p><p class="cantidad-productos">(<%= de0a10.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="pato">
                            </div>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p">$11 a $20</p><p class="cantidad-productos">(<%= de10a20.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="mono">
                            </div>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p">$21 a $30</p><p class="cantidad-productos">(<%= de20a30.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="perro">
                            </div>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p" style="margin-bottom: 0;">Más de $30</p><p class="cantidad-productos" style="margin-bottom: 0;">(<%= de30aX.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="gato" style="margin-bottom: 0;">
                            </div>
                        </section>

                        <section class="lista_filtros_desktop">
                            <h4>Descuentos</h4>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p" style="margin-bottom: 0;"><a href="/productos/perro/promociones">Con 20% OFF</a></p><p class="cantidad-productos" style="margin-bottom: 0;">(<%= checkbox20OffPerro.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="off" style="margin-bottom: 0;">
                            </div>
                        </section>

                        <section class="lista_filtros_desktop">
                            <h4>Marcas</h4>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p"><a href="/productos/marcas/eukanuba">Eukanuba</a></p><p class="cantidad-productos">(<%= checkboxEukanuba.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="eukanuba">
                            </div>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p"><a href="/productos/marcas/royal">Royal Canin</a></p><p class="cantidad-productos">(<%= checkboxRoyal.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="royal">
                            </div>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p"><a href="/productos/marcas/proplan">Pro Plan</a></p><p class="cantidad-productos">(<%= checkboxProPlanPerro.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="proplan">
                            </div>
                            <div class="flex_enlinea_menu">
                                <p class="flex_enlinea_menu-p" style="margin-bottom: 0;">Otras</p><p class="cantidad-productos" style="margin-bottom: 0;">(<%= checkboxOtrasMarcasPerro.length %>)</p><input type="checkbox" class="checkboxFiltroDesktop" id="otrasmarcas" style="margin-bottom: 0;">
                            </div>

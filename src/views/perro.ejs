            <div class="contenedor-filtros-desktop"> <!-- -------- Menus de filtros DESKTOP ------- --> 
                <article class="contenedor_menu_desktop">
                    <div class="contenedor_filtros">

                        <h2 id="h2tituloPerro" class="h2-titulo-desktop">Productos Perro</h2>  <!-- el titulo q se ve en DESKTOP -->
                        <p id="cantidad-prod" class="prod-encontr-desktop"><%= perro.length %> resultados</p>


    <script src="/js/checksboxesCategorias.js"></script>
    <script src="/js/categorias.js"></script>
    <script src="/js/carrito2.js"></script>
    <script src="/js/scroll-up.js"></script>
    <script src="/js/ordenar.js"></script>

</body>
</html>
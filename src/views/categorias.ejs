                <% } %>
                <% if(locals.juguetes){ %>
                    <p class="prod-encontr" style="margin-left: 0;"><%= juguetes.length %> resultados</p>
                <% } %>
                    
                <div class="div-filtros">
                    <p class="btn-icon">Filtrar por<i class="fa-solid fa-chevron-down"></i></p>
                    <div class="contendor_opciones">
                        <p class="opcion-filtro-titulo">Categoría:</p>
                        <p class="opcion-filtro">Accesorios</p>
                        <p class="opcion-filtro">Comida</p>
                        <p class="opcion-filtro">Higiene</p>
                        <p class="opcion-filtro">Juguetes</p>
                        <p class="opcion-filtro"> </p>
                        <p class="opcion-filtro-titulo">Precio:</p>
                        <p class="opcion-filtro">0 a $10</p>
                        <p class="opcion-filtro">$11 a $20</p>
                        <p class="opcion-filtro">$21 a $30</p>
                        <p class="opcion-filtro">Más de $30</p>
                    </div>
                </div>

